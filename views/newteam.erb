        <input type="text" name="pg" placeholder="pg"/> <br>
            Shooting Guard:
        <input type="text" name="sg" placeholder="sg"/> <br>
            Power Forward:
        <input type="text" name="pf" placeholder="pf"/> <br>
            Small Forward:
        <input type="text" name="sf" placeholder="sf"/> <br>
            Center:
        <input type="text" name="c" placeholder="c"/> <br>
        <input type="submit" value="submit"/>

     </form>

 </body>
</html>
